package repli1;

import java.util.*;

public class InterviewPractice {

public static void main(String[] args) {


	String str="OAQAQAAAZKLXLLZZZKKKOOO";
	Map<Character, Integer>map=new LinkedHashMap<>();
	Map<Character, Integer>Unique=new LinkedHashMap<>();
	char [] chars = str.toCharArray();
	
	for (int i = 0; i < chars.length; i++) {
	    if(map.containsKey(chars[i])) {
	    	map.put(chars[i],map.get(chars[i])+1);
	    }else {
	    	map.put(chars[i],1);
	    	Unique.put(chars[i], 1);
	    }
	}
	    
	System.out.println("The Frequancy Of Characters in the string "+map);
	System.out.println("The Unique Character in the String"+Unique);
	
   
	}
}