package repli1;

import java.util.*;

public class InterviewPractice {

public static void main(String[] args) {


	String str="OAQAQAAAZKLXLLZZZKKKOOO";
	Map<Character, Integer>map=new LinkedHashMap<>();
	char [] chars = str.toCharArray();
	for (int i = 0; i < chars.length; i++) {
	    if(map.containsKey(chars[i])) {
	    	map.put(chars[i],map.get(chars[i])+1);
	    }else {
	    	map.put(chars[i],1);
	    	
	    }
	
	    
	}
	List<Map<Character, Integer>>listOfMap=new ArrayList<>();
	listOfMap.add(map);
	
	System.out.println(listOfMap);

    System.out.println(listOfMap.get(0).get('Z'));
	
    Iterator<Map<Character, Integer>>MyI=listOfMap.iterator();
	while(MyI.hasNext()) {
		if(listOfMap.get(0).containsValue(1)) {
			System.out.println(MyI.next());
		}
	}
	
	
  }

}